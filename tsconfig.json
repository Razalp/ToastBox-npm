{
  "compilerOptions": {
    "target": "esnext", 
    "lib": ["dom", "esnext"], 
    "module": "esnext", 
    "moduleResolution": "node", 
    "jsx": "react-jsx", 
    "esModuleInterop": true,
    "strict": true,
    "declaration": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist" 
  },
  "include": ["src/**/*"], 
  "exclude": ["node_modules"]
}
